   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "df = pd.DataFrame()\n",
    "df[\"col\"] = pd.Series([2, np.nan, 5, -1, 0], index=[4, 3, 2, 1, 0])\n",
    "\n",
    "df[\"col2\"] = df[\"col\"].cumsum()\n",
    "df[\"col3\"] = df[\"col\"].sort_index(ascending=True).cumsum()\n",
    "\n",
    "df"
   ]
